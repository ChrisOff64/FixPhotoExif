      // Valeur de retour :
      // 	Aucune
      //
      procédure WriteNewDatetime(taList est un tableau associatif de dateheure, bInEXIF est un booléen, bInModifiedDate est un booléen)
      
      si taList..Occurrence = 0 _OU_ pas (bInEXIF _OU_ bInModifiedDate) alors
      	retour
      FIN
      
      progress(0,taList..Occurrence,"Writing new datetime to file...")
      
      pour TOUT dhTimestamp,sFile DE taList
      	
      	dhE	est une dateheure	= ""
      	si bInEXIF alors
      		// Apply changes
      		ImageInfoModifie(sFile,imgDate,DateHeureVersChaîne(dhTimestamp,"YYYY:MM:JJ HH:mm:SS"))
      		ImageInfoSauve(sFile)
      		ImageInfoFerme(sFile)
      		// Read it back
      		sd	est une chaine	= ImageInfoValeur(sFile,imgDate)
      		dhE..PartieDate		= ChaîneVersDate(sd[[À 10]],"AAAA:MM:JJ")
      		dhE..PartieHeure	= ChaîneVersHeure(sd[[12 À]],"HH:MM:SS")
      	fin
      	
      	dhF est une dateheure	= ""
      	si bInModifiedDate ALORS
      		// Apply changes
      		fDateHeure(sFile,dhTimestamp,fModification)
      		// Read it back
      		dhF = fDateHeure(sFile,"",fModification)
      	FIN
      	
      	ExécuteThreadPrincipal(UpdateFile,sFile,dhF,dhE)
      	
      	si ThreadArrêtDemandé() ALORS
      		sortir
      	FIN
      	
      	CONTINUER:
      	Progress()
      FIN
      
      	// Add result in main table
      	PROCÉDURE INTERNE UpdateFile(sFile est une chaine, dhF est une dateheure, dhE est une dateheure)
      		n est un entier = TableCherche(COL_Filename,sFile,rechercheIdentique)
      		si n>0 alors
      			si datevalide(dhF) alors COL_ModifiedDate[n] = dhF
      			si datevalide(dhE) alors COL_EXIFDate[n] = dhE
      		FIN
      	FIN
      
      FIN:
      
      Progress(0,0,"")
      taskdone("Write")
