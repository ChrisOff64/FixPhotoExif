   -
     name : Progress
     internal_properties : BgAAAAYAAADbEW8Z/kxl81LkpRPFjLp+cFf3YE64vX2DDFGsChtQpc2dMk3OLFgfqbQxFMVSL3zm5THjDyPIMyQsoy91Rcjf9O4RAf6H4YbvKpVyPMH900cwGfwivQ==
     procedure_id : 1284788953488983618
     type_code : 14
     code : |1+
      // Résumé : Display task progression (in main thread).
      // Paramètres :
      //	nIncrement (entier - valeur par défaut=1) : Increment
      //	nMax (entier - valeur par défaut=0) : Max to be reached
      //	sMessage (chaîne ANSI - valeur par défaut="") : Message to display
      // Valeur de retour :
      // 	Aucune
      //
      procédure Progress(nIncrement est un entier = 1, nMax est un entier = 0, sMessage est une chaine = "")
      
      si pas nMax..Defaut ALORS
      	JAUGE_Progress..BorneMax = nMax
      FIN
      
      si pas sMessage..Defaut ALORS
      	LIB_Task = sMessage
      FIN
      
      si nIncrement > 0 alors
      	JAUGE_Progress += nIncrement
      sinon
      	JAUGE_Progress = abs(nIncrement)
      FIN
     type : 458752
   -
     name : ParseFolder
     procedure_id : 1284790564101766829
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Paramètres :
      //	sFolder (chaîne ANSI) : <indiquez ici le rôle de Param1>
      //	bRecursive (booléen) : <indiquez ici le rôle de Param2>
      //	bIgnoreHourPart (booléen) : <indiquez ici le rôle de bIgnoreHourPart>
      //	bCheckAgainstJSON (booléen) : <indiquez ici le rôle de bCheckAgainstJSON>
      // Valeur de retour :
      // 	Aucune
      //
      procédure ParseFolder(sFolder est une chaine, bRecursive est un booléen, bIgnoreHourPart est un booléen, bCheckAgainstJSON est un booléen)
      
      Progress(0,0,"Getting file listing...")
      s est une chaine = fListeFichier(sFolder, bRecursive ? frRécursif sinon frNonRécursif)
      
      progress(0,ChaîneOccurrence(s,RC),"Parsing files...")
      
      // Check for problems with the date
      POUR TOUTE CHAÎNE sFile de s séparée par RC
      	dhJ est une dateheure = ""
      	
      	// Get JSON meta data if available
      	SI fFichierExiste(sFile+".json") ALORS
      		j est un json = fChargeTexte(sFile+".json")
      		
      		// Check the photo taken timestamps
      		SI j.photoTakenTime.timestamp..Existe ALORS
      			dhJ = EpochVersDateHeure(j.photoTakenTime.timestamp)
      		FIN
      		
      		// Check the photo taken timestamps makes no sense, we load the creation time. Better than nothing
      		SI dhJ..Année < 1990 ALORS
      			SI j.creationTime.timestamp..Existe ALORS
      				dhJ = EpochVersDateHeure(j.creationTime.timestamp)
      			FIN
      		FIN
      	FIN
      	
      	// Get file's modified date
      	dhF	est une dateheure	= fDateHeure(sFile,"",fModification)
      	
      	// Get EXIF data (if any)
      	dhE	est une dateheure	= ""	// Date in EXIF
       	dhC est une dateheure	= ""	// Date to use for compare
      	sd	est une chaine		= ImageInfoValeur(sFile,imgDate)
      	si pas ErreurDétectée alors
      		dhE..PartieDate		= ChaîneVersDate(sd[[À 10]],"AAAA:MM:JJ")
      		dhE..PartieHeure	= ChaîneVersHeure(sd[[12 À]],"HH:MM:SS")
      		dhC = dhE
      	sinon
      		dhC = dhF
      	fin
      	
      	SI bCheckAgainstJSON = Faux _OU_ dhJ..PartieDate <> dhC..PartieDate _OU_ (bIgnoreHourPart = Faux _ET_ dhJ..PartieHeure <> dhC..PartieHeure) ALORS
      		ExécuteThreadPrincipal(AddFile,sFile,dhF,dhE,dhJ)
      	FIN
      	
      	si ThreadArrêtDemandé() ALORS
      		sortir
      	FIN
      	
      	CONTINUER:
      	Progress()
      FIN
      
      	// Add result in main table
      	procédure interne AddFile(sFile est une chaine, dhF est une dateheure, dhE est une dateheure, dhJ est une dateheure)
      		TableAjouteLigne(TABLE_Files,sFile,dhF,dhE,dhJ,dhJ)
      	fin
      
      FIN:
      Progress(0,0,"")
      TaskDone("List")
     type : 458752
